            contentType: false,
            /**
            * 必须false才会避开jQuery对 formdata 的默认处理
            * XMLHttpRequest会对 formdata 进行正确的处理
            */
            processData: false,
            dataType: "json",
            beforeSend: function () {
                uploading = true;
            },
            success: function (data) {
                uploading = false;
                alert(data);
            },
            error: function (data) {

                uploading = false;
                alert("上传失败" + data);
            }
        });
    });
</script>