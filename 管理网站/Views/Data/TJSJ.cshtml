                uploading = false;
                alert(data);
            },
            error: function (data) {

                uploading = false;
                alert("上传失败" + data);
            }
        });
    });


    $("#tdcj").on("change", function () {
        if (uploading) {
            alert("文件正在上传中，请稍候");
            return false;
        }
        var formData = new FormData($("#tdcjform")[0]);
        $.ajax({
