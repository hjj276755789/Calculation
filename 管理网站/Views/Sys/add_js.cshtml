<div class="panel panel-border panel-success">
    <div class="panel-heading"><h3 class="panel-title">添加角色</h3></div>
    <div class="panel-body">
        <div class="form-group">
            <label for="exampleInputEmail1">角色名称</label>
            <input class="form-control" id="jsmc" type="text">
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1">角色描述</label>
            <input class="form-control" id="jsms" type="text" placeholder="开发商联系人">
        </div>
        <button class="btn btn-success waves-effect waves-light" type="button" onclick="add_js()">提交</button>
    </div>
</div> <!-- panel -->

<script>
    function add_js() {

        $.ajax({
            url: "/sys/ADD_JSXX",
            type: "post",
            data: { 'jsmc': $("#jsmc").val(), 'jsms': $("#jsms").val() },
            success: function (data) {
                if (data.IsSuccessful) {
                    $('#tjjs').modal('hide');
                    init1();
                }
                else {
                    alert(data.ErrMsg);
                }
            }
        })
    }
</script>